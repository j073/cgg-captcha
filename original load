function onloadTurnstileCallback() {
                turnstile.render('.mputama', {
                    sitekey: '0x4AAAAAAAYh-MitzeYWd4KS',
                    callback: function (token) {
                        window.location.replace("https://jonskie42.github.io/cgg-captcha/amp/");
                        console.log(`Challenge Success ${token}`);
                    },
                });
            };